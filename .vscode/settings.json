{
  "eslint.enable": true,
  "files.exclude": {
    "__**": false,
    ".babelrc": true,
    ".bitmap": false,
    ".git": true,
    ".next": false,
    ".pnp": true,
    ".vscode": true,
    "**/.DS_Store": true,
    "**/.env.development.local": true,
    "**/.env.local": true,
    "**/.env.production.local": true,
    "**/.env.test.local": true,
    "**/.pnp.js": true,
    "**/*.css.map": true,
    "**/*.min.css": true,
    "**/*.min.css.map": true,
    "**/npm-debug.log*": true,
    "**/PG.js": true,
    "**/yarn-debug.log*": true,
    "**/yarn-error.log*": true,
    "bit.json": false,
    "bower.json": true,
    "build": true,
    "coverage": true,
    "node_modules": true,
    "src/App.min.css.map": true,
    "src/App.test.js": true,
    "src/serviceWorker.js": true,
    "static/.DS_Store": false,
    "styles.css": false,
    "styles.css.map": false,
    "styles.min.css": false,
    "styles.min.css.map": false,
    "yarn.lock": true
  },
  "workbench.fontAliasing": "auto",
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "off",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.showIcons": false,
  "workbench.editor.swipeToNavigate": true,
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.tabSizing": "shrink",
  "explorer.openEditors.visible": 6,
  "explorer.sortOrder": "modified",
  "search.showLineNumbers": true,
  "search.useGlobalIgnoreFiles": true,
  "standard.enable": false
}